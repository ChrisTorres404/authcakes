{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec(\\.v[0-9]+(\\.[0-9]+)*)?\\.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "transformIgnorePatterns": [
    "node_modules/(?!(jose)/)"
  ],
  "setupFilesAfterEnv": ["<rootDir>/test-setup.ts"],
  "moduleNameMapper": {
    "^jose$": "<rootDir>/../node_modules/jose/dist/node/cjs/index.js"
  }
}
